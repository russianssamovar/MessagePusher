
<head>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" />
    <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-1.9.0.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>

<div>

    <form method="post" id="diag_form">

        <div class="form-group">
            <label for="AppToken" class="col-sm-2 control-label">AppToken</label>
            <div class="row">
                <div class="col-sm-6">
                    <input type="text" name="AppToken" class="form-control" >
                </div>
            </div>
        </div>
        <div class="form-group">
            <label for="UserKey" class="col-sm-2 control-label">UserKey</label>
            <div class="row">
                <div class="col-sm-6">
                    <input type="text" name="UserKey" class="form-control">
                </div>
            </div>
        </div>
        <div class="form-group">
            <label for="Message" class="col-sm-2 control-label">Message</label>
            <div class="row">
                <div class="col-sm-6">
                    <input type="text" name="MessageString" class="form-control" />
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <input type="button" id="btn" value="Отправить" />
            </div>
        </div>
    </form>
    <div id="result"></div>
</div>
<script>

    $(document).ready(function() {
        $("#btn").click(
            function() {
                sendRequest('diag_form', '/api');
                return false;
            }
        );


        function sendRequest(ajaxForm, url) {
            $.ajax({
                url: url, 
                type: "GET",
                data: $("#" + ajaxForm).serialize(), // Сеарилизуем объект
                success: function (response) { //Данные отправлены успешно
                    $("#result").html("Сообщение отправлено");
                    console.log(response);
                },
                error: function (response) { // Данные не отправлены
                    $("#result").html("Сообщение не отправлено");
                    console.log(response);
                }
            });
        }

        });

</script>